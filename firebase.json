{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
 "codebase": "default",
 "source": "functions",
 "ignore": [
        "venv",
 ".git",
 "firebase-debug.log",
 "firebase-debug.*.log",
 "*.pyc",
 "__pycache__/"
 ]
    }
  ],
  "hosting": {
    "public": ".next",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "main"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}